{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "pnpm run dev-replace-params; vite",
    "dev": "pnpm run dev-replace-params; vite",
    "build": "pnpm run replace-params; vite build",
    "serve": "vite preview",
    "replace-params": "cp src/params/prod-params.js src/params/params.js",
    "clear": "aws s3 rm --recursive s3://neuro-console --profile prod",
    "copy": "aws s3 cp --recursive ./dist s3://neuro-console --profile prod",
    "invalidate_cloudfront": "aws cloudfront create-invalidation --paths '/*' --distribution-id E15YN00AWMADKB --profile prod",
    "deploy": "pnpm run replace-params; pnpm run build; pnpm run clear;  pnpm run copy;  pnpm run invalidate_cloudfront;",
    "dev-replace-params": "cp src/params/dev-params.js src/params/params.js",
    "dev-clear": "aws s3 rm --recursive s3://playingwithml-console --profile dev",
    "dev-copy": "aws s3 cp --recursive ./dist s3://playingwithml-console --profile dev",
    "dev-invalidate_cloudfront": "aws cloudfront create-invalidation --paths '/*' --distribution-id E1NB0ZVBAEVHCH --profile dev",
    "dev-deploy": "pnpm run dev-replace-params; pnpm run build; pnpm run dev-clear;  pnpm run dev-copy; pnpm run dev-invalidate_cloudfront"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "@solidjs/router": "^0.8.0",
    "axios": "^1.3.5",
    "solid-js": "^1.6.10"
  }
}
